{
    "name": "danielmadu/laravel-reactphp",
    "description": "A package to use Laravel with ReactPHP",
    "type": "library",
    "require": {
        "react/event-loop": "^1.0",
        "react/http": "^0.8.3"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Madureira Batista",
            "email": "daniel.madureira.batista@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "ReactPHPLaravel\\Providers\\ServerServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "ReactPHPLaravel\\": "src/"
        }
    }
}
